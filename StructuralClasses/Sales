package StructuralClasses;

public class Sales extends Orders{
	


	// Variables
	
	private double inHouseRevenues;
	private double takeoutRevenues;
	private double deliveryRevenues;
	private double grandTotalRevenues;
	private double totalTyperevenue;
	private double marginalRevenue;
	
	// Constructors
	
	public Sales() {}
	
	public Sales(double inHouseRevenues, double takeoutRevenues, double deliveryRevenues, double grandTotalRevenues,
			double totalTyperevenue, double marginalRevenue) {
		super();
		this.inHouseRevenues = inHouseRevenues;
		this.takeoutRevenues = takeoutRevenues;
		this.deliveryRevenues = deliveryRevenues;
		this.grandTotalRevenues = grandTotalRevenues;
		this.totalTyperevenue = totalTyperevenue;
		this.marginalRevenue = marginalRevenue;
	}

	// Methods
	
	public int compareRevenues(Object firstRevenue, Object secondRevenue)
	{
		// Code stuff
		// Fix the idea of actually comparing the revenues
		
		return -1; // int will reflect higher? could change thsi idea
	}
	
	// Getters and Setters
	
	// In House Revenues
	
	public double getInHouseRevenues() 
	{
		return inHouseRevenues;
	}

	public void setInHouseRevenues(double inHouseRevenues) 
	{
		this.inHouseRevenues = inHouseRevenues;
	}

	// Takeout Revenues
	
	public double getTakeoutRevenues() 
	{
		return takeoutRevenues;
	}

	public void setTakeoutRevenues(double takeoutRevenues) 
	{
		this.takeoutRevenues = takeoutRevenues;
	}

	// Delivery Revenues
	
	public double getDeliveryRevenues() 
	{
		return deliveryRevenues;
	}

	public void setDeliveryRevenues(double deliveryRevenues) 
	{
		this.deliveryRevenues = deliveryRevenues;
	}

	// Grand Total Revenues
	
	public double getGrandTotalRevenues() 
	{
		return grandTotalRevenues;
	}

	public void setGrandTotalRevenues(double grandTotalRevenues) 
	{
		this.grandTotalRevenues = grandTotalRevenues;
	}

	// Total Type Revenue
	
	public double getTotalTyperevenue() 
	{
		return totalTyperevenue;
	}

	public void setTotalTyperevenue(double totalTyperevenue) 
	{
		this.totalTyperevenue = totalTyperevenue;
	}

	// Marginal Revenues
	
	public double getMarginalRevenue() 
	{
		return marginalRevenue;
	}

	public void setMarginalRevenue(double marginalRevenue) 
	{
		this.marginalRevenue = marginalRevenue;
	}

	// To String
	
	@Override
	public String toString() {
		return "Sales [inHouseRevenues=" + inHouseRevenues + ", takeoutRevenues=" + takeoutRevenues
				+ ", deliveryRevenues=" + deliveryRevenues + ", grandTotalRevenues=" + grandTotalRevenues
				+ ", totalTyperevenue=" + totalTyperevenue + ", marginalRevenue=" + marginalRevenue + "]";
	}
	
	

}
